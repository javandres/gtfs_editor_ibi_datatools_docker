# This client ID refers to the UI client in Auth0.
AUTH0_CLIENT_ID: my_auth0_client_id
AUTH0_DOMAIN: mydomain.auth0.com
AUTH0_PUBLIC_KEY: /opt/datatools-server/configurations/auth.pem # uses RS256 signing algorithm
AUTH0_TOKEN: my_auth0_token

DISABLE_AUTH: false
OSM_VEX: http://localhost:1000
SPARKPOST_KEY: your-sparkpost-key
