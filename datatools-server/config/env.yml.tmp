GTFS_DATABASE_USER: postgres
GTFS_DATABASE_PASSWORD: postgres
MONGO_URI: mongodb://datatools-mongodb:27117
MONGO_DB_NAME: catalogue
MONGO_HOST: datatools-mongodb:27117